<?xml version="1.0"?>
<!-- Open_Manipulator Chain -->
<robot name="open_manipulator" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find open_manipulator_description)/urdf/open_manipulator.gazebo.xacro" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find open_manipulator_description)/urdf/materials.xacro" />

  <!-- Transmission macro -->
  <xacro:macro name="SimpleTransmission" params="joint n">
    <transmission name="tran${n}">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${joint}">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="motor${n}">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
  </xacro:macro>

  <!-- World -->
  <link name="world">
  </link>

  <!-- World fixed joint-->
  <joint name="world_fixed" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="link1"/>
  </joint>

  <!-- Link 1 -->
  <link name="link1">
    <visual>
      <origin xyz="0 0 0.027" rpy="${-pi/2} 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link1.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link1.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.016911 -7.3022E-05 -0.0080745" rpy="${-pi/2} 0 0" />
      <mass value="0.14147" />
      <inertia ixx="2.1813E-05" ixy="-1.7015E-08" ixz="-8.9152E-07"
               iyy="2.3447E-05" iyz="-1.5454E-07"
               izz="1.6364E-06" />
    </inertial>
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="link1" />
    <child link="link2" />
    <origin xyz="0 0 0.0515" rpy="0 0 ${-pi/2}" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
  </joint>

  <!-- Transmission 1 -->
  <xacro:SimpleTransmission n="1" joint="joint1" />

  <!--  Link 2 -->
  <link name="link2">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link2.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link2.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.00042334871953208 0.0005762171718733 0.0387383572136045" rpy="0 0 0" />
      <mass value="0.129397139554481" />
      <inertia ixx="3.69350041678783E-05" ixy="-3.89313759320941E-08" ixz="-1.11653037527299E-08"
               iyy="3.68198693987775E-05" iyz="-1.1111795426955E-06"
               izz="1.48030530212799E-07" />
    </inertial>
  </link>

  <!--  Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link2" />
    <child link="link3" />
    <origin xyz="0.026 0 0.05475" rpy="${pi/2} 0 ${pi/2}" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
  </joint>

  <!-- Transmission 2 -->
  <xacro:SimpleTransmission n="2" joint="joint2" />

  <!--  Link 3 -->
  <link name="link3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link3.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link3.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.00634911017847554 0.105401689990389 -0.0262056408085307" rpy="0 0 0" />
      <mass value="0.12120137926006" />
      <inertia ixx="0.000489960675623491" ixy="-5.26672832890087E-05" ixz="1.49822052691775E-07"
               iyy="2.42965312448389E-05" iyz="1.56967790646472E-06"
               izz="0.000482873776104937" />
    </inertial>
  </link>

  <!--  Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link3" />
    <child link="link4" />
    <origin xyz="0.024 0.1684 -0.045" rpy="0 0 ${pi/2}" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
  </joint>

  <!-- Transmission 3 -->
  <xacro:SimpleTransmission n="3" joint="joint3" />

  <!--  Link 4 -->
  <link name="link4">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link4.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link4.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.0107684769865891 -0.0395998807006063 0.0190000000054333" rpy="0 0 0" />
      <mass value="0.0597850138451838" />
      <inertia ixx="1.5907540511103E-05" ixy="2.8291751472122E-06" ixz="-6.22476258071719E-13"
               iyy="6.16523427095478E-06" iyz="-7.18764926930745E-15"
               izz="1.15699642937667E-05" />
    </inertial>
  </link>

  <!--  Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link4" />
    <child link="link5" />
    <origin xyz="0 -0.062 0.019" rpy="${pi/2} ${-pi/2} 0" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
  </joint>

  <!-- Transmission 4 -->
  <xacro:SimpleTransmission n="4" joint="joint4" />

  <!--  Link 5 -->
  <link name="link5">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link5.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link5.stl" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.000218076760146619 -0.000626191256536957 0.0596980339879584" rpy="0 0 0" />
      <mass value="0.0796444353162854" />
      <inertia ixx="6.5346536618451E-05" ixy="1.08804671081198E-08" ixz="3.20425969376465E-07"
               iyy="6.52473697844542E-05" iyz="1.49110423887788E-06"
               izz="3.4317718754864E-07" />
    </inertial>
  </link>

  <!--  Joint 5 -->
  <joint name="joint5" type="revolute">
    <parent link="link5" />
    <child link="link6" />
    <origin xyz="0.019 0 0.088" rpy="${pi/2} 0 ${pi/2}" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
  </joint>

  <!-- Transmission 5 -->
  <xacro:SimpleTransmission n="5" joint="joint5" />

  <!--  Link 6 -->
  <link name="link6">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link6.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link6.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.0107684769865891 0.0395998807006062 -0.0189999999945667" rpy="0 0 0" />
      <mass value="0.0597850138451838" />
      <inertia ixx="1.59075405111031E-05" ixy="2.8291751472122E-06" ixz="6.22476246848532E-13"
               iyy="6.16523427095479E-06" iyz="7.18764844874428E-15"
               izz="1.15699642937667E-05" />
    </inertial>
  </link>

  <!--  Joint 6 -->
  <joint name="joint6" type="revolute">
    <parent link="link6" />
    <child link="link7" />
    <origin xyz="0 0.062 -0.019" rpy="${-pi/2} 0 ${pi}" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
  </joint>

  <!-- Transmission 6 -->
  <xacro:SimpleTransmission n="6" joint="joint6" />

  <!--  Link 7 -->
  <link name="link7">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link7.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link7.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="8.11464269925566E-05 -0.00070824109768956 -0.0220980053972996" rpy="0 0 0" />
      <mass value="0.0741805648173052" />
      <inertia ixx="5.00300817099651E-06" ixy="-2.1246207546751E-07" ixz="2.45442201688391E-08"
               iyy="9.69324841658574E-06" iyz="-2.18474747851751E-06"
               izz="8.88278852334281E-06" />
    </inertial>
  </link>

  <!--  Gripper link -->
  <link name="gripper_link">
    <visual>
      <origin xyz="0 0 -0.027" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link_grip_r.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 -0.027" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link_grip_r.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.0347516179427996 0.010629552145661 -0.0270934437190569" rpy="0 0 0" />
      <mass value="0.0460906192162207" />
      <inertia ixx="1.34126147791776E-05" ixy="-8.10308483393553E-06" ixz="-1.05608290826753E-07"
               iyy="2.13311499424932E-05" iyz="-6.10789237260564E-08"
               izz="1.80372747734964E-05" />
    </inertial>
  </link>

  <!--  Gripper joint -->
  <joint name="gripper" type="revolute">
    <parent link="link7" />
    <child link="gripper_link" />
    <origin xyz="0.027 -0.016 -0.026" rpy="${-pi/2} ${pi/4} ${-pi/2}" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
  </joint>

  <!-- Transmission 7 -->
  <xacro:SimpleTransmission n="7" joint="gripper" />

  <!--  Gripper link sub -->
  <link name="gripper_link_sub">
    <visual>
      <origin xyz="0 0 -0.027" rpy="0 0 ${pi/2}" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link_grip_l.stl" />
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 -0.027" rpy="0 0 ${pi/2}" />
      <geometry>
        <mesh filename="package://open_manipulator_description/meshes/chain_link_grip_l.stl" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.00340216508542759 -0.00576540023107835 -2.1799589910998E-09" rpy="0 0 0" />
      <mass value="0.0356307403873285" />
      <inertia ixx="1.81811062489467E-05" ixy="2.51217685396069E-06" ixz="-1.0866660092931E-08"
               iyy="9.13797195647898E-06" iyz="-1.08688133683192E-08"
               izz="1.03452048539682E-05" />
    </inertial>
  </link>

  <!--  Gripper joint sub -->
  <joint name="gripper_sub" type="revolute">
    <parent link="link7" />
    <child link="gripper_link_sub" />
    <origin xyz="0.027 0.016 -0.026" rpy="${-pi/2} ${pi/4} ${-pi/2}" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi}" upper="${pi}" />
    <mimic joint="gripper" multiplier="-1"/>
  </joint>

  <!-- Transmission 8 -->
  <xacro:SimpleTransmission n="8" joint="gripper_sub" />

  <!-- end effector joint -->
  <joint name="end_effector_joint" type="fixed">
    <origin xyz="0.0 0.0 -0.086" rpy="0 0 0"/>
    <parent link="link7"/>
    <child link="end_effector_link"/>
  </joint>

  <!-- end effector link -->
  <link name="end_effector_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01" />
      </geometry>
      <material name="red"/>
     </visual>

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="0.001"/>
      <inertia ixx="1.0e-06" ixy="0.0" ixz="0.0" iyy="1.0e-06" iyz="0.0" izz="1.0e-06" />
    </inertial>
  </link>

</robot>
